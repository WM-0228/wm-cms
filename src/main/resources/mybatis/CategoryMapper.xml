<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wangming.mapper.CategoryMapper">
	<select id="getById" resultType="category">
		SELECT * FROM cms_category WHERE id = ${value}
	</select>
	
	<select id="getByChannelId" resultType="category">
		SELECT * FROM cms_category WHERE channel_id = ${value}
	</select>
</mapper>